class Solution:
    def isPalindrome(self, head: Optional[ListNode]) -> bool:
        node = ListNode(0)
        cur = node
        temp = head
        while temp:
            cur.next = ListNode(temp.val)
            temp=temp.next
            cur = cur.next
        temp = head
        node = node.next
        while temp:
            if temp.val != node.val:
                return False
            temp = temp.next
            node = node.next
        return True
