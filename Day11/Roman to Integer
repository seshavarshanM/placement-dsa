class Solution:
    def romanToInt(self, s: str) -> int:
        romanMap = {'I': 1, 'V': 5, 'X': 10, 'L': 50,
                'C': 100, 'D': 500, 'M': 1000}
        i =0
        res =0
        while i < len(s):

            if i+1 < len(s) and romanMap[s[i+1]] > romanMap[s[i]]:
                res+= romanMap[s[i+1]] - romanMap[s[i]]
                i+=2

            else:
                res+= romanMap[s[i]]
                i+=1
        return res
